    <div class="container flex items-center justify-center mx-auto pb-12">
        <div class="w-full lg:w-8/12">
            <div class="flex items-center justify-between">
                <h1 class="text-xl font-bold text-gray-700 md:text-2xl">Latest Posts</h1>
            </div>
			<% for(var i = 0; i < posts.length; i++){ %>
			<!--Start Blog Post-->
            <div class="mt-6">
                <div class="max-w-4xl px-10 py-6 mx-auto bg-white rounded-lg shadow-md">
                    <div class="flex items-center justify-between">
						<span class="font-light text-gray-600">
							<%- months[(posts[i]['createdAt']).getMonth()] %> 
							<%-posts[i]['createdAt'].getDay()%>,
							<%- (posts[i]['createdAt']).getFullYear()%>
						</span>
						<div class="flex flex-col md:flex-row">
							<% for(var j = 0; j < posts[i]['Tags'].length; j++){ %>
							<a href="/blog?q=<%- posts[i]['Tags'][j]['id']%>"
								class="px-2 py-1 font-bold text-gray-100 bg-gray-600 rounded hover:bg-gray-500 mx-1 my-1">
								<%- posts[i]['Tags'][j]['name']%>
							</a>
							<% } %>
						</div>
                    </div>
                    <div class="mt-2 text-gray-600">
						<a href="<%- posts[i]['id']%>" class="text-2xl font-bold text-gray-700 hover:underline">
							<%- posts[i]['title']%>
						</a>
							<%-posts[i]['content'].substring(0,256)%>...
                    </div>
                    <div class="flex items-center justify-between mt-4"><a href="<%- posts[i]['id']%>"
                            class="text-cyan-800 hover:underline"><%- posts[i]['title']%></a>
                    </div>
                </div>
            </div>
			<!--End Blog Post-->
			<% } %>
